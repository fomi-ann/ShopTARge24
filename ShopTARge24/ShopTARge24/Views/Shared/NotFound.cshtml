@model Guid

@{
	ViewData["Title"] = "Not Found";
}

<div class=text-center mt-5">
<h1 class="text-danger">404 - Not Found</h1>

<hr />

<p>The item (id: @Model) you where looking for does not exist.</p>
<p>Try looking up something else next time, buddy.</p>

<a asp-action="Index" asp-controller="Home" class="btn btn-secondary">Home</a>
</div>